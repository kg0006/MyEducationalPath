<!DOCTYPE html>
<html>
  <head>
    <title>User page</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <%- include('./header') %>
    <% users.forEach((user) => { %>
      <li>  
        <h2><%= user.name %></h2>
        <h2><%= user.email %></h2>
        <button id="<%=user.id %>" data-action="edite">
          <a href="/edite/<%= user.id %>">edite</a></button>
        <button id="<%=user.id %>" data-action="remove">X</button>
      </li>
    <% }) %>

    <script src="/js/index.js"></script>
  </body>
</html>
